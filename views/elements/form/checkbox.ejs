<%
let count = 0;
options.forEach(function(item) {%>
<div class="form-check">
	<label class="form-check-label" for="<%- name %>[<%- item %>]">
		<input class="form-check-input" type="checkbox" name="<%- name %>[<%- item %>]" id="<%- name %>[<%- count %>]" value="<%- item %>" <%- item === value ? "checked" : "" %>>
		<span class="form-check-sign">
            <span class="check"></span>
        </span>
		<%- item %>
	</label>
</div>
<% count++;}) %>
